{
  "name": "kijewoku-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:poker-tap": "turbo dev --filter=poker-tap",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "create-docs": "turbo create-docs",
    "docs": "turbo docs",
    "create-react-pkg": "turbo gen \"Create a new React component package\"",
    "create-typecript-pkg": "turbo gen \"Create a new TypeScript utility package\""
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/styled-components": "^5.1.34",
    "prettier": "^3.2.5",
    "turbo": "^2.3.3",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "docsify-cli": "^4.4.4",
    "strip-indent": "2.0.0",
    "styled-components": "^6.1.13",
    "uuidv5": "^1.0.0"
  }
}
